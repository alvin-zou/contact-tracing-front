{
  "env": {
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "no-plusplus": 0,
    "no-alert": 2,
    "no-console": 1,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": [0],
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0,
    "no-use-before-define": 1
  }
}
